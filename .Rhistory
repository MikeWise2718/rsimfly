options(rgl.useNULL = TRUE) library(shiny) library(rglwidget) library(rgl) app = shinyApp(   ui = bootstrapPage(       checkboxInput("rescale","Rescale"),       rglwidgetOutput("rglPlot")   ),   server = function(input,output) {     output$rglPlot <- renderRglwidget({       try(rgl.close()) # added following @user2554330       if (input$rescale) aspect3d(1,1,10) else aspect3d(1,1,1)       # High level function following @user2554330: same effect       # plot3d(rnorm(100), rnorm(100), rnorm(100,sd = 0.1),        #     col = rainbow(1000))       spheres3d(rnorm(100),rnorm(100),rnorm(100,sd = 0.1),col = "red",                 radius = 0.1)       axes3d()       rglwidget()     })   }) runApp(app)
options(rgl.useNULL = TRUE) library(shiny) library(rglwidget) library(rgl) app = shinyApp(   ui = bootstrapPage(       checkboxInput("rescale","Rescale"),       rglwidgetOutput("rglPlot")   ),   server = function(input,output) {     output$rglPlot <- renderRglwidget({       try(rgl.close()) # added following @user2554330       if (input$rescale) {         aspect3d(1,1,10)         clr <- "red"       } else {         aspect3d(1,1,1)         clr <- "blue"       }       # High level function following @user2554330: same effect       # plot3d(rnorm(100), rnorm(100), rnorm(100,sd = 0.1),        #     col = rainbow(1000))       spheres3d(rnorm(100),rnorm(100),rnorm(100,sd = 0.1),col = clr,                 radius = 0.1)       axes3d()       rglwidget()     })   }) runApp(app)
